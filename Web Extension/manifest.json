{
  "name": "OBS - 正在播放",
  "version": "1.0.0",
  "description": "用來發送資料給 OBSNowPlayingOverlay 的工具",
  "manifest_version": 3,
  "author": "孤之界 (修改自 Tizhproger)",
  "homepage_url": "https://github.com/konnokai/OBSNowPlayingOverlay",
  "permissions": ["storage"],
  "content_scripts": [
    {
      "matches": [
        "https://*.youtube.com/*",
        "https://*.spotify.com/*",
        "https://*.soundcloud.com/*"
      ],
      "js": ["nowplaying_client.js"]
    }
  ],
  "icons": {
    "16": "16.png",
    "32": "32.png",
    "64": "64.png",
    "128": "128.png"
  }
}
